<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="authorizenet.cred">
        <url>https://apitest.authorize.net/xml/v1/request.api</url>
        <user-id/>
        <password/>
    </service-credential>

    <service-profile service-profile-id="authorizenet.prof">
        <timeout-millis>5000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>5</rate-limit-calls>
        <rate-limit-millis>10000</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>5</cb-calls>
        <cb-millis>10000</cb-millis>
    </service-profile>

    <service service-id="authorizenet.rest.payment">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>AUTHORIZENET</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>authorizenet.prof</profile-id>
        <credential-id>authorizenet.cred</credential-id>
    </service>
</services>
